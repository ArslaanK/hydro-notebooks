    "def decdeg2dms(dd):\n",
    "    mnt,sec = divmod(dd*3600,60)\n",
    "    deg,mnt = divmod(mnt,60)\n",
    "    return deg,mnt,sec\n",
    "def dms2decdeg(deg,mnt,sec):\n",
    "    dd = deg + mnt/60 + sec/3600\n",
    "    return dd"
   ]
